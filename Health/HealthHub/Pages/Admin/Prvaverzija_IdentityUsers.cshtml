@page
@model IdentityUsersModel
@using Microsoft.AspNetCore.Identity
@using Microsoft.AspNetCore.Authorization

<h3 class="bg-secondary text-white text-center p-2">Admin</h3>

<table class="table table-sm table-striped table-bordered">
    <tbody>
        <tr><th>Korisnik</th><td>@Model.AdminUser.UserName</td></tr>
        <tr><th>Email</th><td>@Model.AdminUser.Email</td></tr>
        <tr><th>Telefon</th><td>@Model.AdminUser.PhoneNumber</td></tr>
    </tbody>
</table>




